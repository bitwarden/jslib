{
  "name": "@bitwarden/jslib",
  "version": "0.0.0",
  "description": "Common code used across Bitwarden JavaScript projects.",
  "keywords": [
    "bitwarden"
  ],
  "author": "8bit Solutions LLC",
  "homepage": "https://bitwarden.com",
